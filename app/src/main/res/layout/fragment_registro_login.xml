<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.Registro.RegistroLoginFragment" >

    <EditText
        android:id="@+id/edtPassLog"
        android:layout_width="302dp"
        android:layout_height="37dp"
        android:ems="10"
        android:hint="Ingrese su contraseña"
        android:inputType="text"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edtUserLog"
        app:layout_constraintVertical_bias="0.096" />

    <EditText
        android:id="@+id/edtUserLog"
        android:layout_width="301dp"
        android:layout_height="42dp"
        android:ems="10"
        android:hint="Ingrese su nombre de Usuario"
        android:inputType="text"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edtTelfLog"
        app:layout_constraintVertical_bias="0.117" />

    <EditText
        android:id="@+id/edtTelfLog"
        android:layout_width="343dp"
        android:layout_height="40dp"
        android:ems="10"
        android:hint="ingrese su número de teléfono"
        android:inputType="text"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edtDirLog"
        app:layout_constraintVertical_bias="0.058" />

    <EditText
        android:id="@+id/edtDirLog"
        android:layout_width="343dp"
        android:layout_height="40dp"
        android:ems="10"
        android:hint="Ingrese su dirección"
        android:inputType="text"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edtCorLog"
        app:layout_constraintVertical_bias="0.042" />

    <EditText
        android:id="@+id/edtCorLog"
        android:layout_width="343dp"
        android:layout_height="40dp"
        android:ems="10"
        android:hint="Ingrese su Correo"
        android:inputType="text"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edtAmatLog"
        app:layout_constraintVertical_bias="0.032" />

    <EditText
        android:id="@+id/edtAmatLog"
        android:layout_width="343dp"
        android:layout_height="40dp"
        android:ems="10"
        android:hint="Ingrese su apellido Materno"
        android:inputType="text"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edtApatLog"
        app:layout_constraintVertical_bias="0.022" />

    <EditText
        android:id="@+id/edtApatLog"
        android:layout_width="343dp"
        android:layout_height="40dp"
        android:ems="10"
        android:hint="Ingrese su apellido paterno"
        android:inputType="text"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edtRegNomLog"
        app:layout_constraintVertical_bias="0.024" />

    <EditText
        android:id="@+id/edtRegNomLog"
        android:layout_width="343dp"
        android:layout_height="40dp"
        android:ems="10"
        android:hint="Ingrese su nombre"
        android:inputType="text"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:layout_constraintVertical_bias="0.026" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="343dp"
        android:layout_height="51dp"
        android:text="Crearse Cuenta Nueva"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.069" />

    <Button
        android:id="@+id/btnRegistrarCuenta"
        android:layout_width="199dp"
        android:layout_height="55dp"
        android:text="Registrar Datos"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edtPassLog" />
</androidx.constraintlayout.widget.ConstraintLayout>